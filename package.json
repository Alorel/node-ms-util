{
  "name": "ms-util",
  "version": "0.1.0",
  "description": "For all your millisecond conversion needs",
  "main": "./dist/ms-util.min.js",
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text ava",
    "babelise:index": "babel ./src/index.js | uglifyjs --mangle --compress > ./dist/ms-util.min.js",
    "dist": "npm run babelise:index && npm test",
    "prepublish": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/node-ms-util.git"
  },
  "keywords": [
    "ms",
    "millisecond",
    "convert"
  ],
  "author": "Alorel <a.molcanovas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/node-ms-util/issues"
  },
  "homepage": "https://github.com/Alorel/node-ms-util",
  "devDependencies": {
    "ava": "^0.17",
    "babel-cli": "^6.0",
    "babel-preset-latest": "^6.0",
    "babel-register": "^6.0",
    "coveralls": "^2.11",
    "nyc": "^10.0",
    "uglify-js": "^2.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
